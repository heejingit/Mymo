{
  "name": "mymo-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "src/start.js",
  "build": {
    "appId": "some.id.ofyours",
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "category": "your.app.category.type",
      "iconUrl": "path-to-icon.png"
    },
    "mac": {
      "category": "your.app.category.type",
      "iconUrl": "path-to-icon.png"
    }
  },
  "dependencies": {
    "@ishawnwang/withunmounted": "^1.0.1",
    "@material-ui/core": "^4.7.1",
    "@material-ui/icons": "^4.5.1",
    "bcrypt": "^3.0.7",
    "connected-react-router": "^6.6.1",
    "dotenv": "^8.2.0",
    "electron": "^7.1.2",
    "electron-is-dev": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "knex": "^0.20.3",
    "material-ui": "^0.20.2",
    "material-ui-popup-state": "^1.4.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-popup": "^0.10.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-swipeable-views": "^0.13.3",
    "sqlite3": "https://github.com/mapbox/node-sqlite3/tarball/master"
  },
  "scripts": {
    "electron": "electron .",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "mymo": "npm run start && npm run electron",
    "rebuild": "electron-rebuild -f -w bcrypt",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron-builder": "^21.2.0",
    "electron-rebuild": "^1.8.8"
  }
}
